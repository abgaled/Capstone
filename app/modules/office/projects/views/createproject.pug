extends ../../home/views/navbar.pug

block content
    <div class="right_col" role="main">
        <div class="">
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>Proposal Form<small></small></h2>
                            <div class="clearfix"></div>
                        </div>

                        <form method="POST" action="/office/projects/createproject" class="form-horizontal form-label-left" id="create-proposal">
                            <div class="x_content">
                                <div id="wizard" class="form_wizard wizard_horizontal">
                                    <ul class="wizard_steps">\  
                                        <li>
                                            <a href="#step-1">
                                                <span class="step_no">1</span>
                                                    <span class="step_descr">Statement List<br/>
                                                    <small>Approved statements of barangay</small>
                                                </span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#step-2">
                                                <span class="step_no">2</span>
                                                    <span class="step_descr">Basic<br/>Project Information<br/>
                                                    <small>Basic information regarding the project.</small>
                                                </span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#step-3">
                                                <span class="step_no">3</span>
                                                    <span class="step_descr">Beneficiary Details<br/>
                                                    <small>What details do we need about the beneficiary?</small>
                                                </span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#step-4">
                                                <span class="step_no">4</span>
                                                    <span class="step_descr">Requirements<br/>
                                                    <small>What requirements are needed to <br/>be passed in this project?</small>
                                                </span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#step-5">
                                                <span class="step_no">5</span>
                                                    <span class="step_descr">Timeline<br/>
                                                    <small>Important dates in the project.</small>
                                                </span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#step-6">
                                                <span class="step_no">6</span>
                                                    <span class="step_descr">Budget<br/>
                                                    <small>How much should be in this project?</small>
                                            </span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#step-7">
                                                <span class="step_no">7</span>
                                                    <span class="step_descr">Summary<br/>
                                                    <small>Project proposal overview.</small>   
                                                </span>
                                            </a>
                                        </li>
                                    </ul>

                                    <div id="step-1">
                                        <form class="form-horizontal form-label-left">
                                            <div class="table-responsive">
                                                <table class="table table-striped jambo_table bulk_action">
                                                    <thead>
                                                        <tr class="headings">
                                                            <th>
                                                            </th>
                                                            <th class="column-title">Statement Title </th>
                                                            <th class="column-title">Statement</th>
                                                            <th class="column-title">Categories</th>
                                                            </th>
                                                            <th class="bulk-actions" colspan="7">
                                                                <a class="antoo" style="color:#fff; font-weight:500;">Bulk Actions ( <span class="action-cnt"> </span> ) <i class="fa fa-chevron-down"></i></a>
                                                            </th>
                                                        </tr>
                                                    </thead>

                                                    <tbody id="problemstatement">
                                                        each statement in tbl_problemstatement
                                                            <tr class="even pointer">
                                                                <td class="a-center ">
                                                                    input(type="checkbox" id="ap-statementid" class="ap-statementclass" name="statementsList[]" value=`${statement.int_statementID}` )
                                                                </td>
                                                                td(class="  statementName")
                                                                    value= " " + statement.varchar_statementTitle+"   "
                                                                </td>
                                                                <td class=" statementContent">
                                                                    value=statement.text_statementContent
                                                                </td>
                                                                <td class="  statementCategory">
                                                                    value=statement.varchar_categoryName
                                                                </td>
                                                                </td>
                                                            </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </form>
                                    </div>

                                    <div id="step-2">
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="projectname">Project Name <span class="required">*</span>
                                            </label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <div class="input-group">
                                                    <input type="text" name="projectname" onchange="getName()" id="ap-projectname" required="required" placeholder="Project's name ..." class="form-control col-md-7 col-xs-12">
                                                    <div class="input-group-btn">
                                                        <button type="button" id="btnTitleCheck" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                                            <span class="caret"></span>
                                                        </button>
                                                        <ul id="titleCheck" class="dropdown-menu dropdown-menu-right" role="menu" style="margin-left: -457px; padding: 12px 16px; background-color: #f9f9f9; position:absolute;">
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                            
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12">Project Category</label>
                                            <div class="col-md-6 col-sm-6 col-xs-12" id="ap-projectcateg-intent" style="display:none">
                                            </div>
                                            <div class="col-md-6 col-sm-6 col-xs-12" id="ap-projectcategory-nointent">
                                                <select class="form-control select-multiple" multiple="multiple" id="ap-projectcategory" onclick="getCategory()" name="projectlist[]">
                                                    each category in tbl_category
                                                        option(value=`${category.int_categoryID}`)=category.varchar_categoryName+"   "
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="projectrationale">Project Rationale</label>
                                            <div class="col-md-6 col-sm-6 col-xs-12 ">
                                                <div class="input-group">
                                                    <textarea onchange="getRationale()" class="resizable_textarea form-control" placeholder="Please fill project rationale ..." id="ap-projectrationale" name="projectrationale"></textarea>
                                                    <div class="input-group-btn">
                                                        <button type="button" id="btn-rationale" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                                            <span class="caret"></span>
                                                        </button>
                                                        <ul id="contentDrop" class="dropdown-menu dropdown-menu-right contentDropDown" role="menu" style="margin-left: -457px; padding: 12px 16px; background-color: #f9f9f9; position:absolute;">
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="projectobjective">Project Objective</label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <textarea onchange="getObjective()" class="resizable_textarea form-control" placeholder="Please fill project objective ..." id="ap-projectobjective" name="projectobjective"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="projectdescription">Project Description</label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <textarea onchange="getDescription()" class="resizable_textarea form-control" placeholder="Please fill project description ..." id="ap-projectdescription" name="projectdescription"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12">Select Application Type</label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <select class="form-control" id="ap-apptype" onchange="getAppType()" name="applicationType">
                                                    <option> Select </option>
                                                    <option value='Resident'>Resident</option>
                                                    <option value='Household'>Household</option>
                                                    <option value='Barangay'>Barangay</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    
                                    <div id="step-3">
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12">Select Target Beneficiary</label>
                                            <div class="col-md-6 col-sm-6 col-xs-12" id="ap-projectbeneficiary-intent" style="display:none">
                                            </div>
                                            <div class="col-md-6 col-sm-6 col-xs-12" id="ap-projectbeneficiary-nointent">
                                            <select class="select2_multiple form-control" onclick="getBeneficiary()" id="ap-projectbeneficiary" name="projectbeneficiaries[]" multiple="multiple">
                                                each beneficiary in tbl_beneficiary
                                                    option(value=`${beneficiary.int_beneficiaryID}`)=beneficiary.varchar_beneficiaryName+"   "
                                            </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="allotedslots">Alloted Slot <span class="required">*</span>
                                            </label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <input type="number" onchange="getSlot()" id="ap-allotedslots" name="allotedslots" required="required" placeholder="Number of beneficiary ..." onchange="autobudget()" class="form-control col-md-7 col-xs-12">
                                            </div>
                                        </div>
                                        <hr/>
                                        <div>
                                            <div id="unitmeasurelist" style="display: none">
                                                <select name="unitmeasure[]" class="form-control ap-benefitunit">
                                                    each unit in tbl_unitmeasure
                                                        option(value=`${unit.char_unitSymbol}`)=unit.char_unitSymbol
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <div class="">
                                                    <label class="control-label"> Benefits of the Project </label>
                                                    <button style="pull-right" class="btn btn-info pull-right" id="addBenefit">Add Benefit</button>
                                                </div>
                                                <br/>
                                                <div class="table-responsive">
                                                    <table id="datatable-fixed-header expenseTable" class="table">
                                                        <tbody>
                                                            <tr style="min-height:10%">
                                                                <th width="5%"></th>
                                                                <th width="35%">Benefit</th>
                                                                <th width="10%">Quantity</th>
                                                                <th width="10%">Unit Measure</th>
                                                                <th width="10%">Unit Price</th>
                                                                <th width="20%">Amount</th>
                                                            </tr>
                                                            <tr class="benefit-row beneCounter">
                                                                <th >1</th>
                                                                <td><input type="text" style="width:100%" class="form-control ap-benefitname" name="benefitName[]"></td>
                                                                <td><input type="number" style="width:100%" class="form-control ap-benefitquantity" name="benefitQuantity[]"></td>
                                                                <td>
                                                                    <select name="unitMeasure[]" class="form-control ap-benefitunit">
                                                                        each unit in tbl_unitmeasure
                                                                            option(value=`${unit.char_unitSymbol}`)=unit.char_unitSymbol
                                                                    </select>
                                                                </td>
                                                                <td><input type="number" style="width:100%" class="form-control ap-benefiruprice" name="benefitUnitPrice[]"></td>
                                                                <td>
                                                                    <input type="text" style="width:100%" class="form-control benefitAmounts" name="benefitExAmount[]" readonly>
                                                                    <input type="number" class="benefitAmount" name="benefitAmount[]" hidden>
                                                                </td>
                                                            </tr>
                                                            <tr class="">
                                                                <th ></th>
                                                                <td colspan="4"><b style="float:right">Total Amount:</b></td>
                                                                <td>
                                                                    <input type="text" style="width:100%" class="form-control benefitTot" id="totBeneBudgets" readonly>
                                                                    <input type="number" class="benefitTot" id="totBeneBudget" name="benefitbudget" hidden>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div id="step-4">
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12">Select requirements</label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                            <select class="select2_multiple form-control" onclick="getRequirement()" id="ap-projectrequirement" multiple="multiple" name="projectrequirement[]">
                                                each requirement in tbl_requirement
                                                    option(value=`${requirement.int_requirementID}`)=requirement.varchar_requirementName+"   "
                                            </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div id="step-5">
                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="startApp"> Start of Application<span class="required">*</span>
                                            </label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <div class="form-group has-feedback">
                                                    <input type="date" id="ap-startapp" onchange="getStartApp()" class="pickDate form-control has-feedback-left" name="startApp" aria-describedby="inputSuccess2Status">
                                                    <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                                    <span id="inputSuccess2Status" class="sr-only">(success)</span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="endApp"> End of Application<span class="required">*</span>
                                            </label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <div class="form-group has-feedback">
                                                    <input type="date" id="ap-endapp" onchange="getEndApp()" class="form-control has-feedback-left" name="endApp" aria-describedby="inputSuccess2Status">
                                                    <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                                    <span id="inputSuccess2Status" class="sr-only">(success)</span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="startRel"> Start of Releasing<span class="required">*</span>
                                            </label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <div class="xdisplay_inputx form-group has-feedback">
                                                    <input type="date" id="ap-startrel" onchange="getStartRel()" class="form-control has-feedback-left" name="startRel" aria-describedby="inputSuccess2Status">
                                                    <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                                    <span id="inputSuccess2Status" class="sr-only">(success)</span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="endRel"> End of Releasing<span class="required">*</span>
                                            </label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <div class="form-group has-feedback">
                                                    <input type="date" id="ap-endrel" onchange="getEndRel()" class="datepicker form-control has-feedback-left" name="endRel" aria-describedby="inputSuccess2Status"/>
                                                    <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                                    <span id="inputSuccess2Status" class="sr-only">(success)</span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="closeProj"> Closing of Project<span class="required">*</span>
                                            </label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <div class="form-group has-feedback">
                                                    <input type="date" id="ap-closeproj" onchange="getCloseProj()" class="form-control has-feedback-left" name="closeProj" aria-describedby="inputSuccess2Status">
                                                    <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                                    <span id="inputSuccess2Status" class="sr-only">(success)</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group" hidden id="daychange">
                                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="allotedslot"> Days Between Release Dates<span class="required">*</span>
                                            </label>
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <input type="number" id="betweenDays" required="required" placeholder="Number of days skip..." class="form-control col-md-7 col-xs-12">
                                            </div>
                                        </div>
                                    </div>

                                    <div id="step-6">
                                        <div class="form-group">
                                            <div>
                                                <label class="control-label">Appropriate Budget per Category </label>
                                                <div id="appropriate-budget">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group form-vertical">
                                            <div class="">
                                                <label class="control-label"> Estimated Expense
                                                </label>
                                                <button style="pull-right" class="btn btn-info pull-right" id="addItem">Add Item</button>
                                            </div>
                                            <br/>
                                            <div class="table-responsive">
                                                <table id="datatable-fixed-header expenseTable" class="table">
                                                    <tbody>
                                                        <tr style="min-height:10%">
                                                            <th width="5%"></th>
                                                            <th width="35%">Item Description</th>
                                                            <th width="10%">Quantity</th>
                                                            <th width="10%">Unit Measure</th>
                                                            <th width="10%">Unit Price</th>
                                                            <th width="20%">Amount</th>
                                                        </tr>
                                                        <tr class="expense-row rowCounter">
                                                            <th >1</th>
                                                            <td><input type="text" style="width:100%" class="form-control" name="expenseName[]" id="expName"></td>
                                                            <td><input type="number" style="width:100%" class="form-control quantity" name="expenseQuantity[]" id="expQuan"></td>
                                                            <td>
                                                                <select name="expensemeasure[]" class="form-control" id="expUnit">
                                                                    each unit in tbl_unitmeasure
                                                                        option(value=`${unit.char_unitSymbol}`)=unit.char_unitSymbol
                                                                </select>
                                                            </td>
                                                            <td><input type="number" style="width:100%" class="form-control unitP" name="expenseUnitPrice[]" id="expUPrice"></td>
                                                            <td>
                                                                <input type="text" style="width:100%" class="form-control expenseAmount" id="expAmount" readonly>
                                                                <input type="number" class="expenseEstimatedAmount" name="expenseAmount[]" hidden>
                                                            </td>
                                                        </tr>
                                                        <tr class="expense-row rowCounter">
                                                            <th >2</th>
                                                            <td><input type="text" style="width:100%" class="form-control" name="expenseName[]"></td>
                                                            <td><input type="number" style="width:100%" class="form-control quantity" name="expenseQuantity[]"></td>
                                                            <td>
                                                                <select name="expensemeasure[]" class="form-control">
                                                                    each unit in tbl_unitmeasure
                                                                        option(value=`${unit.char_unitSymbol}`)=unit.char_unitSymbol
                                                                </select>
                                                            </td>
                                                            <td><input type="number" style="width:100%" class="form-control unitP" name="expenseUnitPrice[]"></td>
                                                            <td>
                                                                <input type="text" style="width:100%" class="form-control expenseAmount" readonly>
                                                                <input type="number" class="expenseEstimatedAmount" name="expenseAmount[]" hidden>
                                                            </td>
                                                        </tr>
                                                        <tr class="expense-row rowCounter">
                                                            <th >3</th>
                                                            <td><input type="text" style="width:100%" class="form-control" name="expenseName[]"></td>
                                                            <td><input type="number" style="width:100%" class="form-control quantity" name="expenseQuantity[]"></td>
                                                            <td>
                                                                <select name="expensemeasure[]" class="form-control">
                                                                    each unit in tbl_unitmeasure
                                                                        option(value=`${unit.char_unitSymbol}`)=unit.char_unitSymbol
                                                                </select>
                                                            </td>
                                                            <td><input type="number" style="width:100%" class="form-control unitP" name="expenseUnitPrice[]"></td>
                                                            <td>
                                                                <input type="text" style="width:100%" class="form-control expenseAmount" readonly>
                                                                <input type="number" class="expenseEstimatedAmount" name="expenseAmount[]" hidden>
                                                            </td>
                                                        </tr>
                                                        <tr class="expense-row rowCounter">
                                                            <th >4</th>
                                                            <td><input type="text" style="width:100%" class="form-control" name="expenseName[]"></td>
                                                            <td><input type="number" style="width:100%" class="form-control quantity" name="expenseQuantity[]"></td>
                                                            <td>
                                                                <select name="expensemeasure[]" class="form-control">
                                                                    each unit in tbl_unitmeasure
                                                                        option(value=`${unit.char_unitSymbol}`)=unit.char_unitSymbol
                                                                </select>
                                                            </td>
                                                            <td><input type="number" style="width:100%" class="form-control unitP" name="expenseUnitPrice[]"></td>
                                                            <td>
                                                                <input type="text" style="width:100%" class="form-control expenseAmount" readonly>
                                                                <input type="number" class="expenseEstimatedAmount" name="expenseAmount[]" hidden>
                                                            </td>
                                                        </tr>
                                                        <tr class="expense-row rowCounter">
                                                            <th >5</th>
                                                            <td><input type="text" style="width:100%" class="form-control" name="expenseName[]"></td>
                                                            <td><input type="number" style="width:100%" class="form-control quantity" name="expenseQuantity[]"></td>
                                                            <td>
                                                                <select name="expensemeasure[]" class="form-control">
                                                                    each unit in tbl_unitmeasure
                                                                        option(value=`${unit.char_unitSymbol}`)=unit.char_unitSymbol
                                                                </select>
                                                            </td>
                                                            <td><input type="number" style="width:100%" class="form-control unitP" name="expenseUnitPrice[]"></td>
                                                            <td>
                                                                <input type="text" style="width:100%" class="form-control expenseAmount" readonly>
                                                                <input type="number" class="expenseEstimatedAmount" name="expenseAmount[]" hidden>
                                                            </td>
                                                        </tr>
                                                        <tr class="expense-row rowCounter">
                                                            <th >6</th>
                                                            <td><input type="text" style="width:100%" class="form-control" name="expenseName[]"></td>
                                                            <td><input type="number" style="width:100%" class="form-control quantity" name="expenseQuantity[]"></td>
                                                            <td>
                                                                <select name="expensemeasure[]" class="form-control">
                                                                    each unit in tbl_unitmeasure
                                                                        option(value=`${unit.char_unitSymbol}`)=unit.char_unitSymbol
                                                                </select>
                                                            </td>
                                                            <td><input type="number" style="width:100%" class="form-control unitP" name="expenseUnitPrice[]"></td>
                                                            <td>
                                                                <input type="text" style="width:100%" class="form-control expenseAmount" readonly>
                                                                <input type="number" class="expenseEstimatedAmount" name="expenseAmount[]" hidden>
                                                            </td>
                                                        </tr>
                                                        <tr class="expense-row-copy rowCounter" style="display:none">
                                                            <div id="expenseCopy">
                                                                <td><input type="text" style="width:100%" class="form-control" name="expenseName[]"></td>
                                                                <td><input type="number" style="width:100%" class="form-control quantity" name="expenseQuantity[]"></td>
                                                                <td>
                                                                    <select name="expensemeasure[]" class="form-control">
                                                                        each unit in tbl_unitmeasure
                                                                            option(value=`${unit.char_unitSymbol}`)=unit.char_unitSymbol
                                                                    </select>
                                                                </td>
                                                                <td><input type="number" style="width:100%" class="form-control unitP" name="expenseUnitPrice[]"></td>
                                                                <td>
                                                                    <input type="text" style="width:100%" class="form-control expenseAmount" readonly>
                                                                    <input type="number" class="expenseEstimatedAmount" name="expenseAmount[]" hidden>
                                                                </td>
                                                            </div>
                                                        </tr>
                                                        <tr class="">
                                                            <th ></th>
                                                            <td colspan="4"><b style="float:right">Total Budget:</b></td>
                                                            <td>
                                                                <input type="text" style="width:100%" class="form-control" id="totBudget" readonly>
                                                                <input type="number" class="expenseEstimatedAmount" id="totEstBudget" name="estimatedbudget" hidden>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
