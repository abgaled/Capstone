    script.


        var months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
        n =  new Date();
        y = n.getFullYear();
        m = n.getMonth() + 1;
        d = n.getDate();
        mon = months[n.getMonth()];
        //- currentdate = y + "-" + m + "-" + d;
        currdate =  mon + " " + d + ", " + y;
        currentdate =  m + "-" + d + "-" + y;

        $(document).ready(function () { 
            $('#datatable1').dataTable();
            $('#datatable2').dataTable();
            $('#datatable3').dataTable();
            $('#datatable4').dataTable();
            $('#datatable5').dataTable();
            $('#datatable6').dataTable();
            $('#datatable7').dataTable();

            $('.btn-open').click(function (e) {
                var Id = $(this).val();
                document.getElementById("int_projectID").value = Id
                
                ajaxGetproj();
                    function ajaxGetproj(){
                    $.ajax({
                        type : "POST",
                        url : window.location + "/ajaxgetprojectdetails",
                        data: { ajProjectID: Id},
                        dataType: "json",
                        success: function(tbl_project1){
                            console.log(tbl_project1)

                            $.each(tbl_project1, function (index, value) {
                                console.log(value);
                                if(value.enum_projectStatus == "Created"){
                                    if(value.date_targetStartApp != currdate){
                                        $('#startprojectModal').modal('show');
                                        document.getElementById("projectID").value = Id
                                        document.getElementById("aj-projectDate").innerHTML = value.date_targetStartApp
                                        document.getElementById("aj-current_date").innerHTML = currdate
                                        document.getElementById("current_date").value = currentdate
                                    }
                                    else
                                    {
                                        swal({
                                        title: 'Are you sure?',
                                        text: "You will start the appliction of this project.",
                                        type: 'info',
                                        showCancelButton: true,
                                        confirmButtonColor: '#3085d6',
                                        cancelButtonColor: '#d33',
                                        confirmButtonText: 'Yes, Start appliction!'
                                        }).then((result) => {
                                        if (result.value) {
                                            swal({
                                            text: "Application Opened Successfully",
                                            type: 'success',
                                            confirmButtonColor: '#3085d6',
                                            confirmButtonText: 'OK!'
                                            }).then((result) => {
                                            if (result.value) {
                                                $('#formprojectOPEN').submit();
                                            }                        
                                            })
                                        }                        
                                        })
                                    }
                                }
                                console.log("Success: " );
                            });                                     
                            
                        },
                        error : function(e) {
                            console.log("ERROR: ", e);
                        }
                    });	
                }
            });
                $('#but-sub').on('click',function (e) {
                    var form = $(this).parents('form');
                    e.preventDefault(); //prevent submit
                            
                    swal({
                        title: 'Are you sure?',
                        text: "You sure to open application of project?.",
                        type: 'info',
                        showCancelButton: true,
                        confirmButtonColor: '#3085d6',
                        cancelButtonColor: '#d33',
                        confirmButtonText: 'Yes, Open it!'
                        }).then((result) => {
                        if (result.value) {
                            swal({
                            text: "Application Opened Successfully",
                            type: 'success',
                            confirmButtonColor: '#3085d6',
                            confirmButtonText: 'OK!'
                            }).then((result) => {
                            if (result.value) {
                                $(formstartreason).submit();
                            }                        
                            })
                    }                        
                    })
                });
            //startRELEASING
